     <label for="verbOne">Verb One</label>
     <input id="verbOne" name="verbOne" type="text">

     <label for="adverbOne">Adverb One</label>
     <input id="adverbOne" name="adverbOne" type="text">

     <label for="adjectiveTwo">Adjective Two</label>
     <input id="adjectiveTwo" name="adjectiveTwo" type="text">

     <label for="nounTwo">Noun Two</label>
     <input id="nounTwo" name="nounTwo" type="text">

     <label for="nounThree">Noun Three</label>
     <input id="nounThree" name="nounThree" type="text">

     <label for="adjectiveThree">Adjective Three</label>
     <input id="adjectiveThree" name="adjectiveThree" type="text">

     <label for="verbTwo">Verb Two</label>
     <input id="verbTwo" name="verbTwo" type="text">

     <label for="adverbTwo">Adverb Two</label>
     <input id="adverbTwo" name="adverbTwo" type="text">

     <label for="verbThree">Verb Three</label>
     <input id="verbThree" name="verbThree" type="text">

     <label for="adjectiveFour">Adjective Four</label>
     <input id="adjectiveFour" name="adjectiveFour" type="text">

     <button type="submit">Go!</button>
    </form>
  </body>
</html>

 
